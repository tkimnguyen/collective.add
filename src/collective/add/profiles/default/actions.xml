<?xml version="1.0" encoding="utf-8"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_actions"/>
 <object name="object" meta_type="CMF Action Category">
  <object name="add-folder" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Add Folder</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@add-folder</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Add portal content"/>
   </property>
   <property name="visible">True</property>
   <property name="modal"
      type="text">{&quot;prependContent&quot;: &quot;.portalMessage&quot;, &quot;title&quot;: &quot;Log in&quot;, &quot;width&quot;: &quot;26em&quot;, &quot;actionOptions&quot;: {&quot;redirectOnResponse&quot;: true}}</property>
  </object>
  <object name="add-page" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Add Page</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@add-page</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Add portal content"/>
   </property>
   <property name="visible">True</property>
   <property name="modal"
      type="text">{&quot;prependContent&quot;: &quot;.portalMessage&quot;, &quot;title&quot;: &quot;Log in&quot;, &quot;width&quot;: &quot;26em&quot;, &quot;actionOptions&quot;: {&quot;redirectOnResponse&quot;: true}}</property>
  </object>
 </object>
</object>
